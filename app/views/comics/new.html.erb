<%= form_for @comic, url: {action: "create", method: 'post'} do |f| %>
  <%= f.label :title %>
  <br/>
  <%= f.text_field :title, size: 40 %>
  <br/><br/>
  <%= f.label :hover_text %>
  <br/>
  <%= f.text_field :hover_text, size: 40 %>
  <br/><br/>
  <%= f.label :comic_img %>
  <br/>
  <%= f.file_field :comic_img %>
  <br/><br/>
  <%= f.submit "Create!!!" %>
<% end %>
